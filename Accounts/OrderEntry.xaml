                                        <CheckBox Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" Content="{Cult:LabelText EN_TabC_Inactive, DefaultValue='Inactive'}" Margin="0" Name="ChkCtGeneralInactive" HorizontalAlignment="Right" Padding="0" />
                                        <!--IsChecked="{Binding Path=Active,UpdateSourceTrigger=PropertyChanged}"-->
                                        <CheckBox Content="{Cult:LabelText comm_AO, DefaultValue='AO'}" Name="ChkCTGeneralAO" Margin="0"  Checked="chkAO_Checked" Unchecked="chkAO_Unchecked" HorizontalAlignment="Left"  Visibility="Collapsed" Grid.Row="2" Grid.Column="0" Padding="0" />

