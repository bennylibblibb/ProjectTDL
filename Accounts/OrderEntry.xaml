                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="20"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Text="{Cult:LabelText EN_TabC_Price, DefaultValue='Price'}" Grid.Row="0"  VerticalAlignment="Center" Margin="5,8"/>
                                        <src:ucTBNumber  FontSize="13" Grid.Row="0"  Grid.Column="1" x:Name="txt_Price_CT"  HorizontalContentAlignment="Stretch" VerticalAlignment="Center" Height="30" MinWidth="60" UpBound="99999" DownBound="-99999"/>
                                        <src:ucBtnNumSub  Grid.Column="2" Grid.Row="0" x:Name="btn_Price_CT_down"  Margin="0,20,0,0"  Width="20" Height="20"  TNumBox="{Binding ElementName=txt_Price_CT}"></src:ucBtnNumSub>
                                        <src:ucBtnNumAdd x:Name="btn_Price_CT_up" Grid.Column="2" Margin="0,0,0,25" Width="20" Height="20" TNumBox="{Binding ElementName=txt_Price_CT}"  ></src:ucBtnNumAdd>

                                        <TextBlock Text="{Cult:LabelText EN_TabC_Qty, DefaultValue='Qty'}" Grid.Row="1" Grid.RowSpan="1"  VerticalAlignment="Center"  
Margin="5,8"></TextBlock>

                                        <TextBox Name="txt_Count_CT" TextWrapping="Wrap" InputMethod.IsInputMethodEnabled="False"  Grid.Row="1"  Grid.RowSpan="1" Grid.Column="1" Text="{Binding 
