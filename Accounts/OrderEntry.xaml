                                        <TextBox Grid.Row="3"  Name="cdGeneralRef" TextWrapping="Wrap"  Width="auto" Grid.Column="1" Grid.ColumnSpan="2" Margin="0,0,3,0"/>
                                        <!--Text="{Binding 
Path=Ref,UpdateSourceTrigger=PropertyChanged}"-->
                                    </Grid>
                                    <!--left end-->


                                    <!--right beg-->
                                    <Grid Grid.Column="2" Margin="1,2,2,1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>


                                        <!--===============================================================condition Tab=======================================================-->

                                        <TabControl Grid.RowSpan="3" Name="tbConditionType">
                                            <!--Height="138"-->
                                            <TabItem Header="{Cult:LabelText EN_TabC_A_General, DefaultValue='General'}">
                                                <Border BorderBrush="Silver" BorderThickness="1" Height="Auto" Name="borderType" Width="Auto" >
                                                    <Border.Background>
                                                        <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
