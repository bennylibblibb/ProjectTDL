<GOS:IDelearUser x:Class="GOSTS.OrderEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         Loaded="Window_Loaded" Unloaded="UserControl_Unloaded"
         xmlns:Cult="clr-namespace:GOSTS.GosCulture"
          xmlns:src="clr-namespace:GOSTS.Accounts"   xmlns:GOS="clr-namespace:GOSTS" >
    <!--MinWidth="312" MinHeight="250"-->
    <UserControl.Resources>
        <GOS:ZeroConvertor x:Key="zerotoblank"></GOS:ZeroConvertor>
        <GOS:IntValidate x:Key="invalidator"></GOS:IntValidate>
        <Style x:Name="CustomStyle" TargetType="ToggleButton">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <StackPanel>
                            <Image Name="img" Source="/GOSTS;component/Images/ribbtn_lock_open.png"/>
                            <TextBlock Text="ToggleButton"/>
                        </StackPanel>
                        <ControlTemplate.Triggers>
                            <Trigger Property="ToggleButton.IsChecked" Value="True">
                                <Setter TargetName="img" Property="Source" Value="/GOSTS;component/Images/ribbtn_lock_locked.png"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style TargetType="Button"  x:Key="accTradeBtn">
            <Setter Property="Button.Foreground" Value="#FFF7F5F5"></Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Control.Foreground" Value="Black" />
                </Trigger>
            </Style.Triggers>
        </Style>

    </UserControl.Resources>
    <!--<Viewbox>-->
    <Grid Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Rectangle Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="1" Grid.RowSpan="2" HorizontalAlignment="Stretch" Height="auto"  Stroke="Black" VerticalAlignment="Stretch" Width="auto">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <!--<GradientStop Color="#FFF1F1F1" Offset="0.042"/>
                    <GradientStop Color="#FFA7B4B3" Offset="0.925"/>-->
                    <GradientStop Color="#FFF2F2F8" Offset="0"/>
                    <GradientStop Color="#FFB8BBD0" Offset="0.5"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="1" Grid.RowSpan="1" HorizontalAlignment="Stretch" Height="auto"  Stroke="Black" VerticalAlignment="Stretch" Width="auto">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <!--<GradientStop Color="#FFF1F1F1" Offset="0.042"/>
                    <GradientStop Color="#FF51B441" Offset="0.925"/>-->
                    <GradientStop Color="#FFF2F2F8" Offset="0"/>
                    <GradientStop Color="#FFB8BBD0" Offset="0.5"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>

        <StackPanel HorizontalAlignment="Stretch"  VerticalAlignment="Top" Width="auto" Orientation="Horizontal">
            <Grid Name="gdAccInfo">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1"></RowDefinition>
                    <RowDefinition Height="32"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"></ColumnDefinition>
                    <ColumnDefinition Width="auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <GOS:AEUseCOB x:Name="cbbUsers" Grid.Row="1" Grid.Column="0"></GOS:AEUseCOB>
            </Grid>
            <!--<GOS:AEUseCOB FontSize="10" x:Name="cbbUsers" Grid.Row="0" Grid.Column="0"></GOS:AEUseCOB>
            <Button Height="20" Width="20" Margin="10,0,0,0">
                <Image Source="/GOSTS;component/Images/ribbtn_user.png" Stretch="Fill"/>
            </Button>
            <Label Name="lbUserID"  Content="" Padding="25,5,5,5" FontSize="10"/>-->
        </StackPanel>
        <Grid Grid.Row="1" Name="gdProdInfo">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>

            </Grid.ColumnDefinitions>
            <ToggleButton Height="20" Width="20" Visibility="Collapsed" VerticalContentAlignment="Stretch" Margin="10,0,0,0" Name="tbuCode" >
            </ToggleButton>
            <Label Grid.Column="1" Content="{Cult:LabelText ET_ProdCod, DefaultValue='ID'}" Width="29" Padding="3,5,3,5"/>
            <TextBox Grid.Column="2" Name="txt_OrderID" CharacterCasing="Upper" TextWrapping="Wrap" Text="{Binding Path=ProductCode,UpdateSourceTrigger=PropertyChanged}" MinWidth="60" MaxWidth="120" FontSize="12" Margin="2,2,0,4" TextChanged="txt_OrderID_TextChanged"  KeyDown="txt_OrderID_KeyDown_1"/>
            <Label  Grid.Column="3"  Name="lbProdName" Content="{Binding Path=ProductName,UpdateSourceTrigger=PropertyChanged}" Padding="0,5,0,5" FontSize="12"/>

        </Grid>
        <!--<StackPanel HorizontalAlignment="Stretch"  VerticalAlignment="Top" Width="auto" Orientation="Horizontal" Grid.Row="1">
        </StackPanel>-->


        <DockPanel HorizontalAlignment="Stretch" Height="auto" VerticalAlignment="Stretch" Width="auto" Grid.Row="2">
            <TabControl Name="tabc" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,2,0,0">
