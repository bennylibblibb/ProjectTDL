                    <TabItem.Header>
                        <TextBlock TextAlignment="Left" Text="{Cult:LabelText ET_TabNormal, DefaultValue='Normal Trade'}" ></TextBlock>
                    </TabItem.Header>
                    <Border BorderBrush="Silver" BorderThickness="1" Height="auto" Name="borderNT" Width="auto" >
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
                                <GradientStop Color="#FFF2F2F8" Offset="0"/>
                                <GradientStop Color="#FFB8BBD0" Offset="0.5"/>
                            </LinearGradientBrush>
                        </Border.Background>

                        <Viewbox Stretch="Fill">
                            <!-- test  StretchDirection="UpOnly"beg-->
                            <!--begin-->
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextBlock.FontSize="12">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" MinHeight="90"/>
                                    <RowDefinition Height="3"/>
                                    <RowDefinition Height="35"/>
                                </Grid.RowDefinitions>

                                <!-- 上方输入控件等begin-->
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="auto" MinWidth="120"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="auto"/>
                                    </Grid.ColumnDefinitions>
                                    <!--left beg-->
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="20"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Text="{Cult:LabelText ET_TbN_Price, DefaultValue='Price'}" Grid.Row="0" Grid.RowSpan="2"  VerticalAlignment="Center" Margin="5,8"/>
                                        <src:ucTBNumber  FontSize="13" Grid.Row="0" Grid.RowSpan="2" Grid.Column="1" x:Name="txt_Price_NT"  Width="auto" HorizontalContentAlignment="Stretch" VerticalAlignment="Center" Height="30" MinWidth="60" Margin="0,2,1,2" UpBound="99999" DownBound="-99999"/>
                                        <!--<TextBox FontSize="13" Grid.Row="0" Grid.RowSpan="2" Grid.Column="1" Name="txt_Price_NT" TextWrapping="Wrap"  KeyUp="tbDecimal_KeyUp" Width="auto" HorizontalContentAlignment="Stretch" VerticalAlignment="Center" Height="30" MinWidth="60" Margin="0,2,1,2">
                                        </TextBox>-->
                                        <src:ucBtnNumSub  Grid.Column="2" Grid.Row="1" x:Name="btnPrDow"  Width="20" Height="20"  TNumBox="{Binding ElementName=txt_Price_NT}"></src:ucBtnNumSub>
                                        <src:ucBtnNumAdd x:Name="btnPrAdd" Grid.Column="2" Width="20" Height="20" TNumBox="{Binding ElementName=txt_Price_NT}"></src:ucBtnNumAdd>
                                        <!--<src:ContinuousButtonAdd UpBound="99999" Step="1" x:Name="btnPrAdd" Grid.Column="2" Width="20" Height="20" 
TBoxNum="{Binding ElementName=txt_Price_NT}" />-->
                                        <!--<src:ContinuousButtonSub Grid.Column="2" Step="1" DownBound="-99999" Grid.Row="1" x:Name="btnPrDow"  Width="20" 
Height="20" TBoxNum="{Binding ElementName=txt_Price_NT}" />-->

                                        <TextBlock Text="{Cult:LabelText ET_TbN_Qty, DefaultValue='Qty'}" Grid.Row="2" Grid.RowSpan="2" VerticalAlignment="Center"  
Margin="5,8"></TextBlock>

                                        <TextBox Name="txt_Count_NT" TextWrapping="Wrap" InputMethod.IsInputMethodEnabled="False"  Grid.Row="2"  Grid.RowSpan="2" Grid.Column="1" Text="{Binding 
Path=Count,UpdateSourceTrigger=PropertyChanged}" Width="auto"   HorizontalContentAlignment="Stretch" Height="30" Margin="0,2,1,2"/>

                                        <src:ContinuousButtonAdd UpBound="999999" x:Name="btnQtyUp" Grid.Row="2" Grid.Column="3" Width="20" Height="20" TBoxNum="{Binding ElementName=txt_Count_NT}" Margin="0,2,0,3" HorizontalAlignment="Left"/>
                                        <src:ContinuousButtonSub x:Name="btnQtyDown" Grid.Row="3" Grid.Column="3" DownBound="1" Width="20"
Height="20" TBoxNum="{Binding ElementName=txt_Count_NT}" Margin="0"/>
                                    </Grid>
                                    <!--left end-->

                                    <!--right beg-->
                                    <Grid Grid.Column="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="5"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <ComboBox HorizontalAlignment="Left" Name="ntValid"  Width="75" Grid.Row="0" Grid.Column="0">
                                            <ComboBoxItem Tag="Today" IsSelected="True">
                                                <TextBlock Text="{Cult:LabelText ValidType_today, DefaultValue='Today'}"/>
                                            </ComboBoxItem>
                                            <ComboBoxItem Tag="FAK">
                                                <TextBlock Text="{Cult:LabelText ValidType_fak, DefaultValue='FAK'}"/>
                                            </ComboBoxItem>
                                            <ComboBoxItem Tag="FOK">
                                                <TextBlock Text="{Cult:LabelText ValidType_fok, DefaultValue='FOK'}"/>
                                            </ComboBoxItem>
                                            <ComboBoxItem Tag="GTC">
                                                <TextBlock Text="{Cult:LabelText ValidType_gtc, DefaultValue='GTC'}"/>
                                            </ComboBoxItem>
                                            <ComboBoxItem Tag="specTime">
                                                <TextBlock Text="{Cult:LabelText ValidType_specDate, DefaultValue='SPEC DATE'}"/>
                                            </ComboBoxItem>
                                        </ComboBox>
                                        <DatePicker HorizontalAlignment="Left" Name="ntSecTime" SelectedDate="{Binding 
Path=dtSpecialTime,UpdateSourceTrigger=PropertyChanged}" Grid.Row="0" Grid.Column="2" IsEnabled="False" Grid.ColumnSpan="2" />

                                        <CheckBox Content="{Cult:LabelText comm_AO, DefaultValue='AO'}" Name="chkAO" Margin="0"  Checked="chkAO_Checked" Unchecked="chkAO_Unchecked" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1"  Grid.Column="0" Padding="0" />

                                        <CheckBox Content="{Cult:LabelText ET_TbN_Inactive, DefaultValue='Inactive'}" Name="chkNTActive"  Margin="0" VerticalAlignment="Center" HorizontalAlignment="Right" Grid.Row="1"  Grid.Column="2" Padding="0" IsChecked="{Binding Path=Active,UpdateSourceTrigger=PropertyChanged}"/>

                                        <Grid Grid.Row="2"  Grid.Column="0" Grid.ColumnSpan="3" HorizontalAlignment="Stretch">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                                <ColumnDefinition Width="*"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Label Content="{Cult:LabelText ET_TbN_Ref, DefaultValue='Ref'}"  Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                            <TextBox Name="txt_NT_ref" TextWrapping="Wrap" Text="{Binding 
Path=Ref,UpdateSourceTrigger=PropertyChanged}" Width="auto" Grid.Column="1" Margin="0,0,3,0"
HorizontalContentAlignment="Stretch" />
                                        </Grid>

                                    </Grid>
                                    <!--right end-->
                                </Grid>
                                <!-- 上方输入控件等end-->

                                <!-- 下方提交按钮begin -->
                                <Grid Grid.Row="2" VerticalAlignment="Stretch">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition MinWidth="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition MinWidth="10"/>
                                        <ColumnDefinition Width="auto"/>
                                        <ColumnDefinition MinWidth="10"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition ></RowDefinition>
                                    </Grid.RowDefinitions>

                                    <Button Name="btn_NTbuy" Width="65" Content="{Cult:LabelText ET_TbN_Buy, DefaultValue='Buy'}" FontSize="13" FontWeight="Bold" Style="{StaticResource 
accTradeBtn}"   Grid.Column="1"  Click="btn_NTbuy_Click" Focusable="True" >
                                        <Button.Background>
                                            <ImageBrush ImageSource="/GOSTS;component/Images/img_button_blue.png"/>
                                        </Button.Background>
                                    </Button>
                                    <Button Name="btn_NTsell" Width="65" MinHeight="30" Content="{Cult:LabelText ET_TbN_Sell, DefaultValue='Sell'}" FontSize="13" FontWeight="Bold" Style="{StaticResource 
accTradeBtn}" Grid.Row="0" Grid.Column="3" Click="btn_NTsell_Click" Focusable="True"  >
                                        <Button.Background>
                                            <ImageBrush ImageSource="/GOSTS;component/Images/img_button_red.png"/>
                                        </Button.Background>
                                    </Button>
                                </Grid>
                                <!-- 下方提交按钮end -->
                            </Grid>
                            <!--end -->


                            <!-- test end-->

                        </Viewbox>
                    </Border>
                </TabItem>
                <TabItem Height="20" FontSize="11" Name="tbQuickTrade">
                    <TabItem.Header>
                        <TextBlock TextAlignment="Left" Text="{Cult:LabelText EN_TabQuickTrade, DefaultValue='Quick Trade'}" Height="12" VerticalAlignment="Top" ></TextBlock>
                    </TabItem.Header>
                    <Border BorderBrush="Silver" BorderThickness="1"  Name="borderQT" Width="auto" >
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.504,1.5" StartPoint="0.504,0.03">
                                <GradientStop Color="#FFF2F2F8" Offset="0"/>
                                <GradientStop Color="#FFB8BBD0" Offset="0.5"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Viewbox Stretch="Fill" >
                            <!--<StackPanelStretchDirection="UpOnly">-->

                            <Grid TextBlock.FontSize="12">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="{Cult:LabelText EN_TabQ_Ticks, DefaultValue='Ticks'}" VerticalAlignment="Center" VerticalContentAlignment="Center"/>
                                <!--追價點數-->
                                <!--<Button Grid.Column="1" Focusable="False"  Height="25" Width="25">
                                    <Image Source="/GOSTS;component/Images/ribbtn_down.png" Stretch="Fill"/>
                                </Button>-->
                                <src:ContinuousButtonSub  Grid.Column="1" x:Name="btnTickDown" DownBound="0" Width="25" Height="25" TBoxNum="{Binding ElementName=txtQTradeTicks}" />
                                <TextBox TextWrapping="Wrap" Name="txtQTradeTicks" InputMethod.IsInputMethodEnabled="False"  Text="{Binding Path=Ticks,UpdateSourceTrigger=PropertyChanged}" Width="auto" MinWidth="45" Grid.Column="2" HorizontalContentAlignment="Stretch"/>
                                <!--<Button Grid.Column="3" Focusable="False" Height="25" Width="25" >
                                    <Image Source="/GOSTS;component/Images/ribbtn_up.png"  Stretch="Fill"/>
                                </Button>-->
                                <src:ContinuousButtonAdd UpBound="99999" x:Name="btnTickUp" Grid.Column="3" Focusable="False" Height="25" Width="25" TBoxNum="{Binding ElementName=txtQTradeTicks}" />

                                <Label Content="{Cult:LabelText EN_TbN_Type, DefaultValue='Type'}" VerticalAlignment="Center" VerticalContentAlignment="Center" Grid.Column="4" HorizontalAlignment="Left"/>
                                <!--種     類-->
                                <Label Content="{Cult:LabelText EN_TabQ_Qty, DefaultValue='Qty'}" Grid.Row="1"/>
                                <!--手數-->
                                <!--<Button Grid.Column="1" Grid.Row="1" Focusable="False" Height="25" Width="25" >
                                    <Image  Source="/GOSTS;component/Images/ribbtn_down.png"  Stretch="Fill"/>
                                </Button>-->
                                <src:ContinuousButtonSub   Grid.Column="1" Grid.Row="1" Focusable="False" DownBound="1" Width="25" Height="25" TBoxNum="{Binding ElementName=txtQTradeCount}" />
                                <TextBox TextWrapping="Wrap" Name="txtQTradeCount" InputMethod.IsInputMethodEnabled="False"  Text="{Binding Path=Count,UpdateSourceTrigger=PropertyChanged}" Width="auto" MinWidth="45" Grid.Column="2" Grid.Row="1" HorizontalContentAlignment="Stretch" />
                                <!--<Button Grid.Column="3" Grid.Row="1" Focusable="False" Height="25" Width="25" >
                                    <Image Source="/GOSTS;component/Images/ribbtn_up.png"  Stretch="Fill"/>
                                </Button>-->
                                <src:ContinuousButtonAdd UpBound="99999"   Grid.Column="3" Grid.Row="1" Focusable="False"  Height="25" Width="25" TBoxNum="{Binding ElementName=txtQTradeCount}" />
                                <ComboBox Grid.Column="4" Grid.Row="1" HorizontalAlignment="Right" Name="cmbQuickValid">
                                    <ComboBoxItem Tag="Today" IsSelected="True">
                                        <TextBlock Text="{Cult:LabelText ValidType_today, DefaultValue='Today'}"/>
                                    </ComboBoxItem>
                                    <ComboBoxItem Tag="FaK">
                                        <TextBlock Text="{Cult:LabelText ValidType_fak, DefaultValue='FAK'}"/>
                                    </ComboBoxItem>
                                    <ComboBoxItem Tag="FoK">
                                        <TextBlock Text="{Cult:LabelText ValidType_fok, DefaultValue='FOK'}"/>
                                    </ComboBoxItem>
                                </ComboBox>

                                <UniformGrid  Columns="2" Grid.Row="2" Grid.ColumnSpan="5">
                                    <Button Name="btnQuickBuy"  FontSize="11" Click="btnQuickBuy_Click_1" FontWeight="Bold" Margin="10,5" Padding="1,3" Style="{StaticResource accTradeBtn}" Focusable="True" >
                                        <TextBlock><TextBlock Text="{Cult:LabelText EN_TabQ_BtnHitBuy, DefaultValue='Hit Buy'}"/> <!--追價買入-->  <TextBlock Text="{Binding Path=Ask}"></TextBlock><LineBreak/><!--追價至-->(<TextBlock Text="{Cult:LabelText EN_TabQ_BtnHitBuy_To, DefaultValue='To'}"/><TextBlock Text="{Binding Path=AskTo}"></TextBlock>)</TextBlock>
                                        <Button.Background>
                                            <ImageBrush ImageSource="/GOSTS;component/Images/img_button_blue.png"/>
                                            <!--<Image  Source="Images/img_button_blue.png" Stretch="None"/>-->
                                        </Button.Background>
                                    </Button>
                                    <Button Name="btnQTradeSell" Click="btnQTradeSell_Click_1" FontSize="11" FontWeight="Bold" Style="{StaticResource accTradeBtn}" Grid.Column="1" Margin="10,5" Padding="1,3" Focusable="True" >
                                        <!--#FFF5EDED-->
                                        <TextBlock><!--追價沽出--><TextBlock Text="{Cult:LabelText EN_TabQ_BtnHitSell, DefaultValue='Hit Sell'}"/><TextBlock Text="{Binding Path=Bid}"></TextBlock><LineBreak/>(<!--追價至--><TextBlock Text="{Cult:LabelText EN_TabQ_BtnHitSell_To, DefaultValue='To'}"/><TextBlock Text="{Binding Path=BidTo}"></TextBlock>)</TextBlock>
                                        <Button.Background>
                                            <ImageBrush ImageSource="/GOSTS;component/Images/img_button_red.png"/>
                                        </Button.Background>
                                        <!--<Image  Source="Images/img_button_red.png" Stretch="None"/>-->
                                    </Button>
                                </UniformGrid>

                            </Grid>

                            <!--</StackPanel>-->
                        </Viewbox>
                    </Border>
                </TabItem>
                <TabItem Visibility="Visible" Height="20" FontSize="11">
